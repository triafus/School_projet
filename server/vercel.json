{
  "version": 2,
  "projectId": "<TON_SERVER_VERCEL_PROJECT_ID>",
  "orgId": "<TON_VERCEL_ORG_ID>",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
